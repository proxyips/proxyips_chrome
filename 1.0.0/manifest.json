{
   "background": {
      "scripts": [ "js/jquery-3.2.1.min.js", "js/background.js" ]
   },
   "browser_action": {
      "default_icon": {
         "19": "images/icon19.png",
         "38": "images/icon38.png"
      },
      "default_popup": "popup.html",
      "default_title": "ProyIps for Chrome"
   },
   "content_security_policy": "script-src 'self' https://chrome.proxyips.net; object-src 'self'",
   "description": "Manage your Proxies",
   "icons": {
      "128": "images/android-chrome-192x192.png",
      "16": "images/icon16.png",
      "24": "images/icon24.png",
      "48": "images/icon48.png",
      "96": "images/icon96.png"
   },
   "manifest_version": 2,
   "name": "Proxy IPs",
   "permissions": [ "proxy", "storage", "\u003Call_urls>", "webRequest", "webRequestBlocking" ],
   "short_name": "ProxyIps",
   "version": "1.0.0"

}
